{
  "name": "@sequenced_app/app",
  "private": true,
  "version": "1.0.9",
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:full": "npm run build && npm run sync",
    "sync": "npx @capacitor/cli sync",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "host": "vite --host",
    "open:ios": "npx @capacitor/cli open ios",
    "open:ios:full": "npm run build:full && npm run open:ios",
    "open:android": "npx @capacitor/cli open android",
    "open:android:full": "npm run build:full && npm run open:android",
    "open:electron": "npx @capacitor/cli open @capacitor-community/electron",
    "run:ios": "npx @capacitor/cli run ios",
    "run:android": "npx @capacitor/cli run android",
    "build:ios": "npm run build:full && npm run run:ios",
    "build:android": "npm run build:full && npm run run:android",
    "build:electron": "npm run build:full && npm run open:electron",
    "resources": "capacitor-resources -p android,ios",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@capacitor-community/admob": "^6.0.0",
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor/android": "^6.0.0",
    "@capacitor/core": "^6.1.2",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/local-notifications": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@headlessui/react": "^2.0.4",
    "@heroicons/react": "^2.1.3",
    "@tanstack/react-query": "^5.40.1",
    "@types/electron": "^1.6.10",
    "@types/node": "^20.14.2",
    "electron-squirrel-startup": "^1.0.1",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^6.0.0",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "electron": "^30.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.38",
    "react-router-dom": "^6.23.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.13"
  }
}
